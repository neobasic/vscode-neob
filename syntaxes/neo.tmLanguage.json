{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "NeoBASIC",
	"scopeName": "source.neob",
	"fileTypes": [ "neob" ],
	"patterns": [
		{ "include": "#directives" },
		{ "include": "#declarations" },
		{ "include": "#statements" },
		{ "include": "#comments" }
	],
	"repository": {
		"directives": {
			"patterns": [ { "include": "#magic-comments" } ]
		},
		"declarations": {
			"patterns": [
				{ "include": "#module-clause" },
				{ "include": "#const-clause" }
			]
		},
		"statements": {
			"patterns": [ { "include": "#magic-statements" } ]
		},
		"comments": {
			"patterns": [
				{
					"name": "comment.line.number-sign.neob",
					"begin": "(#)(?=\\w)(\\w+)",
					"beginCaptures": {
						"1": { "name": "punctuation.definition.comment.neob meta.hashtag.neob" },
						"2": { "name": "meta.hashtag.neob" }
					},
					"end": "$",
					"patterns": [
						{
							"name": "meta.hashtag.neob",
							"match": "#\\w+"
						}
					]
				},
				{
					"name": "comment.line.number-sign.neob",
					"begin": "(#)(?![#!?$])",
					"beginCaptures": { "1": { "name": "punctuation.definition.comment.neob" } },
					"end": "$",
					"patterns": [
						{
							"name": "meta.hashtag.neob",
							"match": "#\\w+"
						}
					]
				},
				{
					"name": "comment.block.neob",
					"begin": "##(?!#)",
					"end": "##(?!#)",
					"captures": { "0": { "name": "punctuation.definition.comment.neob" } },
					"patterns": [
						{
							"name": "meta.hashtag.neob",
							"match": "#\\w+"
						}
					]
				},
				{
					"name": "comment.block.documentation.neob",
					"begin": "###",
					"end": "###",
					"captures": { "0": { "name": "punctuation.definition.comment.neob" } },
					"patterns": [
						{
							"name": "meta.hashtag.neob",
							"match": "#\\w+"
						}
					]
				}
			]
		},
		"magic-comments": {
			"patterns": [
				{
					"name": "comment.line.shebang.neob meta.interpreter-directive.neob",
					"match": "\\A(#!)[^#\\\\]*(?=\\\\|#|$)",
					"captures": { "1": { "name": "punctuation.definition.comment.shebang.neob" } }
				},
				{
					"name": "comment.line.shebang.neob meta.pragma-directive.neob",
					"match": "^(#!)[^#\\\\]*(?=\\\\|#|$)",
					"captures": { "1": { "name": "punctuation.definition.comment.shebang.neob" } }
				},
				{
					"name": "comment.line.woodstock.neob meta.canary-test.neob",
					"match": "^(#\\?)[^#\\\\]*(?=\\\\|#|$)",
					"captures": { "1": { "name": "punctuation.definition.comment.woodstock.neob" } }
				},
				{
					"name": "comment.line.sherlock.neob meta.shell-lookup.neob",
					"match": "^(#\\$)[^#\\\\]*(?=\\\\|#|$)",
					"captures": { "1": { "name": "punctuation.definition.comment.sherlock.neob" } }
				}
			]
		},
		"module-clause": {
			"patterns": [
				{
					"comment": "MODULE CLAUSE",
					"begin": "\\b(module)\\s+",
					"beginCaptures": { "1": { "name": "keyword.module.neob" } },
					"end": "(?!\\G)",
					"patterns": [
						{
							"match": "\\d\\w*",
							"name": "invalid.illegal.identifier.neob"
						},
						{
							"match": "\\w+",
							"name": "entity.name.type.module.neob"
						}
					]
				}
			]
		},
		"const-clause": {
			"comment": "CONSTANT ASSIGNMENT WITH CONST KEYWORD",
			"patterns": [
				{
					"comment": "ONE-LINER CONST ASSIGNMENT",
					"match": "(?:(?<=\\bconst\\b)(?:\\s*)(\\b[\\w\\.]+(?:\\,\\s*[\\w\\.]+)*)(?:\\s*)([\\w\\.]+)?(?:\\s*)(=|:=|::)(?:\\s*)((?:[^\\,]+)(?:\\,\\s*[^,]+)*))",
					"captures": {
						"1": {
							"patterns": [
								{ "include": "#declaration-delimiters" },
								{
									"match": "\\w+",
									"name": "variable.other.constant.neob"
								}
							]
						},
						"2": {
							"patterns": [
								{ "include": "#type-declarations" },
								{
									"match": "\\w+",
									"name": "entity.name.type.neob"
								}
							]
						},
						"3": { "patterns": [ { "include": "#assignment-operators" } ] },
						"4": { "patterns": [ { "include": "#expressions" } ] }
					}
				}
			]
		},
		"magic-statements": {
			"patterns": [
				{
					"name": "punctuation.definition.atom.rubber-duck.neob",
					"match": "^(@)([a-zA-Z0-9_]*)?(=)([^#\\\\]*)(?=\\\\|#|$)",
					"captures": {
						"1": { "name": "punctuation.definition.atom.rubber-duck.begin.neob" },
						"2": { "name": "punctuation.definition.atom.rubber-duck.$2.neob" },
						"3": { "name": "punctuation.definition.atom.rubber-duck.end.neob" },
						"4": { "name": "meta.rubber-duck.neob" }
					}
				},
				{
					"name": "punctuation.definition.atom.tracer-bird.neob",
					"match": "^(@)([a-zA-Z0-9_]*)?(>)([^#\\\\]*)(?=\\\\|#|$)",
					"captures": {
						"1": { "name": "punctuation.definition.atom.tracer-bird.begin.neob" },
						"2": { "name": "punctuation.definition.atom.tracer-bird.$2.neob" },
						"3": { "name": "punctuation.definition.atom.tracer-bird.end.neob" },
						"4": { "name": "meta.tracer-bird.neob" }
					}
				}
			]
		},
		"keywords": {
			"patterns": [
				{ "include": "#reserved-keywords" },
				{ "include": "#predeclared-identifiers" },
				{ "include": "#optional-monads" },
				{ "include": "#value-constructors" },
				{ "include": "#standard-datatypes" },
				{ "include": "#standard-library" }
			]
		},
		"reserved-keywords": {
			"patterns": [
				{
					"comment": "IMPORT CONTROL KEYWORDS",
					"name": "keyword.control.import.$1.neob",
					"match": "\\b(use|as|of|include|interface|socket|http|https|rest|soap|grpc|amqp|ftp|smtp|imap|pop3)\\b"
				},
				{
					"comment": "FLOW CONTROL KEYWORDS",
					"name": "keyword.control.flow.$1.neob",
					"match": "\\b(do|with|defer|next|redo|break|retry|otherwise|continue|fallthrough|return|yield|go|to|switch|when|default|await|async|thread|task|node)\\b"
				},
				{
					"comment": "CONDITIONAL CONTROL KEYWORDS",
					"name": "keyword.control.conditional.$1.neob",
					"match": "\\b(if|then|elif|else|unless|match|case|try|catch)\\b"
				},
				{
					"comment": "LOOPING CONTROL KEYWORDS",
					"name": "keyword.control.looping.$1.neob",
					"match": "\\b(loop|times|for|each|step|while|until)\\b"
				},
				{
					"comment": "TESTING CONTROL KEYWORDS",
					"name": "keyword.control.test.$1.neob",
					"match": "\\b(invar|unit|from|once|data|call|hide|show|into|pass|past|fail)\\b"
				},
				{
					"comment": "TOP SENTENCE DECLARATION KEYWORDS",
					"name": "storage.type.$1.neob",
					"match": "\\b(module|const|val|var|let|type|def|defn|undef|fact|feed|fmap|func|sub|rpc|extern|event|enum|struct|proto|trait|class|get|set)\\b"
				},
				{
					"comment": "BOOLEAN OPERATORS",
					"name": "keyword.operator.boolean.$1.neob",
					"match": "\\b(not|and|or|xor|nand|nor|nxor)\\b"
				},
				{
					"comment": "MISCELLANEOUS OPERATORS",
					"name": "keyword.operator.other.$1.neob",
					"match": "\\b(typeof|instanceof|sizeof|is|in|between|like|new|del)\\b"
				}
			]
		},
		"standard-datatypes": {
			"patterns": [
				{
					"comment": "PRIMITIVE LOGICAL DATA TYPES",
					"name": "storage.type.logic.$1.neob",
					"match": "\\b(Bool(8|16|32|64|128)?)\\b"
				},
				{
					"comment": "PRIMITIVE NUMERICAL DATA TYPES",
					"name": "storage.type.numeric.$1.neob",
					"match": "\\b(Number|Dig|Bit|Oct|Hex|Nibble|Byte|Nat(8|16|32|64|128)?|Bignat|Int(8|16|32|64|128)?|Bigint|Real(16|32|64|128)?|Bigreal|Dec(1|2|3|4|5|6|7|8)|Decimal|Money|Bigdecimal|Ratio(8|16|32|64|128)?|Complex(16|32|64|128)?|Quatern(16|32|64|128)?)\\b"
				},
				{
					"comment": "PRIMITIVE TEMPORAL DATA TYPES",
					"name": "storage.type.time.$1.neob",
					"match": "\\b(Date|Elapse)\\b"
				},
				{
					"comment": "PRIMITIVE CHARACTER DATA TYPES",
					"name": "storage.type.char.$1.neob",
					"match": "\\b(Ascii|Char)\\b"
				},
				{
					"comment": "PRIMITIVE SEQUENCE DATA TYPES",
					"name": "storage.type.sequence.$1.neob",
					"match": "\\b(Str|Regex|Binary)\\b"
				},
				{
					"comment": "PRIMITIVE SHELL DATA TYPES",
					"name": "storage.type.sequence.$1.neob",
					"match": "\\b(Path|Uri|Inet)\\b"
				},
				{
					"comment": "PRIMITIVE MISCELLANEOUS DATA TYPES",
					"name": "storage.type.other.$1.neob",
					"match": "\\b(auto|void|atom|)\\b"
				},
				{
					"comment": "PRIMITIVE COMPOSITE DATA TYPES",
					"name": "storage.type.composite.$1.neob",
					"match": "\\b(Range|Map)\\b"
				},
				{
					"comment": "PRIMITIVE ARRAY DATA TYPES",
					"name": "storage.type.composite.$1.neob",
					"match": "\\b(Array|Tuple|List|Dict|Chan|Vec|Mat|Set|Queue|Deque|Xml|Table|Memo)\\b"
				}
			]
		},
		"standard-library": {
			"patterns": [
				{
					"comment": "BUILT-IN MATH FUNCTIONS",
					"name": "support.function.math.$1.neob",
					"match": "\\b(round|sin|cos|tan|log|log2|log10)\\b"
				},
				{
					"comment": "BUILT-IN TEXT FUNCTIONS",
					"name": "support.function.text.$1.neob",
					"match": "\\b(len|[lr]?trim|indexOf)\\b"
				},
				{
					"comment": "BUILT-IN TIME FUNCTIONS",
					"name": "support.function.time.$1.neob",
					"match": "\\b()\\b"
				},
				{
					"comment": "BUILT-IN STANDARD FUNCTIONS",
					"name": "support.function.stdl.$1.neob",
					"match": "\\b(enter|print)\\b"
				},
				{
					"comment": "BUILT-IN NUMERICAL CONVERTERS",
					"name": "support.converter.numeric.$1.neob",
					"match": "\\b(abs|ceil|floor)\\b"
				},
				{
					"comment": "BUILT-IN TEXT CONVERTERS",
					"name": "support.converter.text.$1.neob",
					"match": "\\b(upper|lower|titled|trim|ltrim|rtrim|pascal|camel|snake|kebab)\\b"
				},
				{
					"comment": "BUILT-IN TEMPORAL CONVERTERS",
					"name": "support.converter.time.$1.neob",
					"match": "\\b(year|month|day|week|workday|hour|minute|second|milli|micro|nano|tick|tzone|ago|ahead)\\b"
				},
				{
					"comment": "BUILT-IN FACT CONVERTERS",
					"name": "support.converter.fact.$1.neob",
					"match": "\\b(all|any|one|two|nil)\\b"
				},
				{
					"comment": "BUILT-IN SHELL FACTS",
					"name": "support.fact.shell.$1.neob",
					"match": "\\b(exist|ready)\\b"
				},
				{
					"comment": "BUILT-IN CONSOLE MACROS",
					"name": "support.macro.console.$1.neob",
					"match": "\\b(at|scan|entry|echo|alert|till|since|timely|cancel|play|cls)\\b"
				},
				{
					"comment": "BUILT-IN SHELL MACROS",
					"name": "support.macro.shell.$1.neob",
					"match": "\\b(cd|pwd|ls|mkdir|rmdir|touch|rm|cp|mv|rename|chmod|chown|chgrp)\\b"
				},
				{
					"comment": "BUILT-IN ERROR-HANDLING MACROS",
					"name": "support.macro.error.$1.neob",
					"match": "\\b(raise|panic)\\b"
				}
			]
		},
		"predeclared-identifiers": {
			"patterns": [
				{
					"comment": "RESERVED LANGUAGE VARIABLES",
					"name": "variable.language.$1.neob",
					"match": "\\b(this|iota|its|top|end|it|self|super)\\b"
				}
			]
		},
		"optional-monads": {
			"patterns": [
				{
					"comment": "LANGUAGE RESULT OPTIONAL MONAD",
					"name": "constant.language.result.$1.neob",
					"match": "\\b(Okay|Fail)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "LANGUAGE MAYBE OPTIONAL MONAD",
					"name": "constant.language.maybe.$1.neob",
					"match": "\\b(None)\\b|\\b(Some)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "LANGUAGE EITHER OPTIONAL MONAD",
					"name": "constant.language.either.$1.neob",
					"match": "\\b(Yea|Nay)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "LANGUAGE STREAM OPTIONAL MONAD",
					"name": "constant.language.stream.$1.neob",
					"match": "\\b(EoT)\\b|\\b(Data)\\b\\(([^)]+)\\)"
				}
			]
		},
		"value-constructors": {
			"patterns": [
				{
					"comment": "LOGICAL VALUE CONSTRUCTORS",
					"name": "constant.language.logic.$1.neob",
					"match": "\\b(False|True)\\b"
				},
				{
					"comment": "NUMERICAL VALUE CONSTRUCTORS",
					"name": "constant.language.numeric.$1.neob",
					"match": "\\b(Zero|MinValue|MaxValue|NaN|NegativeInfinity|PositiveInfinity)\\b|\\b(Nonzero)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "TEMPORAL VALUE CONSTRUCTORS",
					"name": "constant.language.time.$1.neob",
					"match": "\\b(Epoch|Eon|Yesterday|Today|Now|Tomorrow)\\b|\\b(LocalDate|LocalDateTime|OffsetDate|OffsetDateTime|ZonedDate|ZonedDateTime)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "CHARACTER VALUE CONSTRUCTORS",
					"name": "constant.language.time.$1.neob",
					"match": "\\b(Null)\\b|\\b(Letter|Mark|Digit|Punctuation|Symbol|Separator|Other|NonPrintable)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "SEQUENCE VALUE CONSTRUCTORS",
					"name": "constant.language.sequence.$1.neob",
					"match": "\\b(Blank)\\b|\\b(Nonblank)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "FILESYSTEM VALUE CONSTRUCTORS",
					"name": "constant.language.path.$1.neob",
					"match": "\\b(NullDevice)\\b|\\b(Folder|File|LinkFile|PipeFile|SocketFile|BlockDevice|CharDevice)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "INET VALUE CONSTRUCTORS",
					"name": "constant.language.inet.$1.neob",
					"match": "\\b(Ipv4|Ipv6)\\b\\(([^)]+)\\)"
				},
				{
					"comment": "URI VALUE CONSTRUCTORS",
					"name": "constant.language.uri.$1.neob",
					"match": "\\b(Url|Urn)\\b\\(([^)]+)\\)"
				}
			]
		},
		"operators": {
			"patterns": [
				{ "include": "#unary-operators" },
				{ "include": "#binary-operators" },
				{ "include": "#ternary-operators" },
				{ "include": "#assignment-operators" }
			]
		},
		"unary-operators": {
			"comment": "NOTE THAT THE ORDER HERE IS VERY IMPORTANT!",
			"patterns": [
				{
					"match": "\\+(?!\\+)",
					"name": "keyword.operator.positive.neob"
				},
				{
					"match": "\\-(?!\\-)",
					"name": "keyword.operator.negative.neob"
				},
				{
					"match": "\\+\\+",
					"name": "keyword.operator.increment.neob"
				},
				{
					"match": "\\-\\-",
					"name": "keyword.operator.decrement.neob"
				},
				{
					"match": "~~|~(?!~)",
					"name": "keyword.operator.negation.neob"
				},
				{
					"match": "\\^\\^|\\^(?!\\^)",
					"name": "keyword.operator.sort.neob"
				},
				{
					"match": "===|=(?![=])",
					"name": "keyword.operator.clone.neob"
				},
				{
					"match": "\\*\\*|\\*/|\\*\\!",
					"name": "keyword.operator.exponentiation.neob"
				},
				{
					"match": "\\.\\.\\.",
					"name": "keyword.operator.spread.neob"
				},
				{
					"comment": "BOOLEAN OPERATORS",
					"name": "keyword.operator.boolean.$1.neob",
					"match": "\\b(not)\\b"
				},
				{
					"comment": "MISCELLANEOUS OPERATORS",
					"name": "keyword.operator.other.$1.neob",
					"match": "\\b(typeof|instanceof|sizeof|new|del)\\b"
				}
			]
		},
		"binary-operators": {
			"comment": "NOTE THAT THE ORDER HERE IS VERY IMPORTANT!",
			"patterns": [
				{
					"match": "<<\\-|(?<!<)<\\-",
					"name": "keyword.operator.pop.neob"
				},
				{
					"match": ":",
					"name": "keyword.operator.mapping.neob"
				},
				{
					"match": "\\|>",
					"name": "keyword.operator.pipeline.neob"
				},
				{
					"match": "=>>",
					"name": "keyword.operator.bind.neob"
				},
				{
					"match": "\\->(?!>)",
					"name": "keyword.operator.subtype.neob"
				},
				{
					"match": "=>(?!>)",
					"name": "keyword.operator.implicit-return.neob"
				},
				{
					"match": "`",
					"name": "keyword.operator.fmap.neob"
				},
				{
					"match": ";",
					"name": "keyword.operator.modifier.neob"
				},
				{
					"match": "(&>|&&|&(?![&>^=\\s]))",
					"name": "keyword.operator.thread.neob"
				},
				{
					"match": "(==|!=|<=|>=|<(?!<)|>(?!>))",
					"name": "keyword.operator.relational.neob"
				},
				{
					"match": "(\\?%|!%|<=>|\\?:)",
					"name": "keyword.operator.comparison.neob"
				},
				{
					"match": "(!!|!\\?|!(?![=%?!|])|\\?\\?|\\?(?![?%:]))",
					"name": "keyword.operator.coalescing.neob"
				},
				{
					"match": "(\\+|\\-|\\*|/|\\\\\\\\|÷|%|\\*\\*|\\*/|\\*!)",
					"name": "keyword.operator.arithmetic.neob"
				},
				{
					"match": "(%[\\*/\\+\\-\\^])",
					"name": "keyword.operator.arithmetic.percentage.neob"
				},
				{
					"match": "(&(?!\\^)|&\\^|\\^|\\||<<|>>|>>>)",
					"name": "keyword.operator.arithmetic.bitwise.neob"
				},
				{
					"comment": "BOOLEAN OPERATORS",
					"name": "keyword.operator.boolean.$1.neob",
					"match": "\\b(and|or|xor|nand|nor|nxor)\\b"
				},
				{
					"comment": "MISCELLANEOUS OPERATORS",
					"name": "keyword.operator.other.$1.neob",
					"match": "\\b(is|in|between|like)\\b"
				}
			]
		},
		"ternary-operators": {
			"comment": "NOTE THAT THE ORDER HERE IS VERY IMPORTANT!",
			"patterns": [
				{
					"match": "(>?(\\.\\.)(?!\\.)<?)(\\w+):",
					"name": "keyword.operator.interval.neob"
				}
			]
		},
		"assignment-operators": {
			"comment": "NOTE THAT THE ORDER HERE IS VERY IMPORTANT!",
			"patterns": [
				{
					"match": "(=|:=|::)",
					"name": "keyword.operator.assignment.single.neob"
				},
				{
					"match": "(>>>=|<<=|>>=|<<-|<-|<\\||\\*\\*=|\\*/=|\\*=|/=|%%=|÷=|%\\+=|%-=|%=|\\+=|-=|&\\^=|&=|\\^=|\\|=)",
					"name": "keyword.operator.assignment.compound.neob"
				}
			]
		},
		"punctuations": {
			"patterns": [ { "include": "#declaration-delimiters" } ]
		},
		"declaration-delimiters": {
			"comment": "CONSTANTS, VALUES, VARIABLES ARE SEPARATED BY , IN DECLARATION",
			"patterns": [
				{
					"match": "\\,",
					"name": "punctuation.other.comma.neob"
				}
			]
		},
		"type-declarations": {
			"comment": "INCLUDES ALL TYPE DECLARATIONS",
			"patterns": [ { "include": "#standard-datatypes" } ]
		},
		"expressions": {
			"comment": "INCLUDES ALL POSSIBLE VALUES OF EXPRESSIONS",
			"patterns": [
				{ "include": "#identifier" },
				{ "include": "#literals" }
			]
		},
		"identifier": {
			"patterns": [
				{
					"name": "variable.other.neob",
