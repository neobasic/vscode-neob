        "foreground": "#6FEBA5"
      }
    },

    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F56213"
      }
    },
    {
      "scope": "constant.numeric.decimal",
      "settings": {
        "foreground": "#F51E13"
      }
