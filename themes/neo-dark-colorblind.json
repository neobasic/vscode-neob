      "settings": {
        "foreground": "#94CEF8"
      }
    },
    {
      "scope": "string.quoted.double",
      "settings": {
        "foreground": "#A8F173"
      }
    },
    {
      "scope": "string.quoted.double.block",
      "settings": {
        "foreground": "#49F60B"
      }
    },
    {
      "scope": "string.quoted.single",
      "settings": {
        "foreground": "#EBEF72"
      }
    },
    {
      "scope": "string.quoted.single.block",
      "settings": {
        "foreground": "#E9F50C"
      }
