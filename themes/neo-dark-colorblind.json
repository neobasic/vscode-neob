{
  "name": "Neo Dark Colorblind",
  "semanticHighlighting": true,
  "colors": {
    "editorBracketHighlight.unexpectedBracket.foreground": "#34EBB4"
  },
  "tokenColors": [
    {
      "scope": "source.neo",
      "settings": {
        "foreground": "#E346C9"
      }
    },

    {
      "scope": [
        "comment.line.number-sign.neo",
        "punctuation.definition.comment.neo"
      ],
      "settings": {
        "foreground": "#7FEA05"
      }
    },

    {
      "scope": ["comment.block.neo"],
      "settings": {
        "foreground": "#72C217"
      }
    },

    {
      "scope": ["comment.block.documentation.neo"],
      "settings": {
        "foreground": "#71A13A"
      }
    },

    {
      "scope": ["meta.hashtag.neo"],
      "settings": {
        "foreground": "#8E90E6"
      }
    },

    {
      "scope": [
        "comment.line.shebang.neo",
        "punctuation.definition.comment.shebang.neo"
      ],
      "settings": {
        "foreground": "#0C9898"
      }
    },
    {
      "scope": ["meta.interpreter-directive.neo"],
      "settings": {
        "foreground": "#00ffff"
      }
    },

    {
      "scope": ["meta.pragma-directive.neo"],
      "settings": {
        "foreground": "#7DBCEA"
      }
    },

    {
      "scope": [
        "comment.line.woodstock.neo",
        "punctuation.definition.comment.woodstock.neo"
      ],
      "settings": {
        "foreground": "#82880B"
      }
    },
    {
      "scope": ["meta.canary-test.neo"],
      "settings": {
        "foreground": "#ECF44C"
      }
    },

    {
      "scope": [
        "comment.line.sherlock.neo",
        "punctuation.definition.comment.sherlock.neo"
      ],
      "settings": {
        "foreground": "#6E6E6E"
      }
    },
    {
      "scope": ["meta.shell-lookup.neo"],
      "settings": {
        "foreground": "#AEAEAE"
      }
    },

    {
      "scope": ["punctuation.definition.atom.rubber-duck"],
      "settings": {
        "foreground": "#D9EE36"
      }
    },

    {
      "scope": [
        "punctuation.definition.atom.rubber-duck.neo",
        "punctuation.definition.atom.rubber-duck.begin.neo",
        "punctuation.definition.atom.rubber-duck.end.neo"
      ],
      "settings": {
        "foreground": "#DB5B6F"
      }
    },
    {
      "scope": ["meta.rubber-duck.neo"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2DDD68"
      }
    },

    {
      "scope": [
        "punctuation.definition.atom.tracer-bird.neo",
        "punctuation.definition.atom.tracer-bird.begin.neo",
        "punctuation.definition.atom.tracer-bird.end.neo"
      ],
      "settings": {
        "foreground": "#0B83F3"
      }
    },
    {
      "scope": ["meta.tracer-bird.neo"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9A9C40"
      }
    },
    {
      "scope": [
        "punctuation.definition.atom.tracer-bird.trace.neo",
        "punctuation.definition.atom.tracer-bird.debug.neo",
        "punctuation.definition.atom.tracer-bird.info.neo",
        "punctuation.definition.atom.tracer-bird.warn.neo",
        "punctuation.definition.atom.tracer-bird.error.neo",
        "punctuation.definition.atom.tracer-bird.fatal.neo"
      ],
      "settings": {
        "foreground": "#25C3CB"
      }
    },

    {
      "scope": "keyword",
      "settings": {
        "foreground": "#E346C9"
      }
    },

    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#E346C9"
      }
    },

    {
      "scope": "support.function",
      "settings": {
        "foreground": "#E346C9"
      }
    },
    {
      "scope": "support.converter",
      "settings": {
        "foreground": "#E346C9"
      }
    },
    {
      "scope": "support.fact",
      "settings": {
        "foreground": "#E346C9"
      }
    },
    {
      "scope": "support.macro",
      "settings": {
        "foreground": "#E346C9"
      }
    },

    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#E346C9"
      }
    },
    {
      "scope": "constant.language",
