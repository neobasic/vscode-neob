      }
    },

    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#3AD4E8"
      }
    },
    {
      "scope": "keyword.other.unit.binary",
      "settings": {
        "foreground": "#75E1F0"
      }
    },
    {
      "scope": "keyword.other.unit.octal",
      "settings": {
        "foreground": "#164173"
      }
    },
    {
      "scope": "keyword.other.unit.hexadecimal",
      "settings": {
        "foreground": "#03DBF7"
      }
    },
    {
      "scope": "keyword.other.unit.exponent",
