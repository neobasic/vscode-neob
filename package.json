{
  "name": "neobasic",
  "displayName": "NeoBASIC",
  "description": "NeoBASIC language support for Visual Studio Code.",
  "version": "0.0.1",
  "publisher": "[WE THE DEVS]",
  "author": {
    "name": "Raul Bras",
    "url": "https://teknolista.substack.com"
  },
  "license": "MIT",
  "readme": "README.md",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1b1f23",
    "theme": "dark"
  },
  "main": "./out/extension.js",
  "homepage": "https://www.neobasic.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/neobasic/vscode-neob"
  },
  "bugs": {
    "url": "https://github.com/neobasic/vscode-neob/issues"
  },
  "qna": "https://github.com/neobasic/vscode-neob/discussions/categories/q-a",
  "engines": {
    "vscode": "^1.80.0"
  },
  "enableTelemetry": false,
  "keywords": [
    "neobasic",
    "neob",
    "basic",
    "code",
    "coding",
    "programming",
    "language",
    "grammar",
    "syntax",
    "highlighting",
    "theme",
    "dark",
    "light",
    "protanopia",
    "colorblind"
  ],
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "neob",
        "aliases": [
          "NeoBASIC",
          "neobasic",
          "neob",
          "basic"
        ],
        "extensions": [
          ".neob"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "neob",
        "scopeName": "source.neob",
        "path": "./syntaxes/neob.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "Neo Light (Colorblind)",
        "uiTheme": "vs",
        "path": "./themes/neob-light-colorblind.json"
      },
      {
        "label": "Neo Dark (Colorblind)",
        "uiTheme": "vs-dark",
        "path": "./themes/neob-dark-colorblind.json"
      }
    ]
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/vscode": "^1.102.0",
    "vscode": "^1.1.34"
  }
}
